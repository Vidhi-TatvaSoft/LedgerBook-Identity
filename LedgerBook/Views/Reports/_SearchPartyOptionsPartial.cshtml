@using DataAccessLayer.ViewModels;
@model List<PartyViewModel>;

@if (Model.Count != 0)
{
    @foreach (var party in Model)
    {
        <div class="w-100 p-2 ps-2 border-bottom" data-partyid="@party.PartyId" onclick="searchByParty(@party.PartyId,'@party.PartyName')">@party.PartyName</div>
    }
}
